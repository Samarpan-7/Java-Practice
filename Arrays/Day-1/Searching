//Searching
class Main {
    public static void main(String[] args) {
        int[] a = {10, 12, 32, 38, 39, 44};
        int key = 38;
        int pos = -1;
        for (int i = 0; i < a.length; i++) {
            if (a[i] == key) {
                pos = i;
                break;
            }
        }
        if (pos != -1)
            System.out.println("Element found at position: " + (pos + 1));
        else
            System.out.println("Element not found");
    }
}
